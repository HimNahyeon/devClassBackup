	<%
	/*
	2*1 = 2			3*1 = 3			4*1= 4			.....
	2*2 = 4 ....    이런형식으로
	*/
	int result = 0;
	
	for(int i = 1; i <=9; i++ ){		
		for(int j = 2; i<10; i++){
			result = i*j;
	%>			
			<!-- 분리를 할수가있어! 신기해! -->
	
	<%= j %> * <%= i %> = <%= result %>
			
	<%
		}
	%>
	<br>	
	<%
	}
	%>
